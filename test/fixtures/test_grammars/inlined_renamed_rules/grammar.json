{
  "name": "inlined_renamed_rules",

  "extras": [
    {"type": "PATTERN", "value": "\\s"}
  ],

  "inline": [
    "expression"
  ],

  "rules": {
    "statement": {
      "type": "SEQ",
      "members": [
        {"type": "SYMBOL", "name": "expression"},
        {"type": "STRING", "value": ";"}
      ]
    },

    "expression": {
      "type": "CHOICE",
      "members": [
        {"type": "SYMBOL", "name": "call_expression"},
        {"type": "SYMBOL", "name": "member_expression"},
        {
          "type": "RENAME",
          "value": "variable_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },

    "call_expression": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {"type": "SYMBOL", "name": "expression"},
          {"type": "STRING", "value": "("},
          {"type": "SYMBOL", "name": "expression"},
          {"type": "STRING", "value": ")"},
        ]
      }
    },

    "member_expression": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {"type": "SYMBOL", "name": "expression"},
          {"type": "STRING", "value": "."},
          {
            "type": "RENAME",
            "value": "property_name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          }
        ]
      }
    },

    "identifier": {"type": "PATTERN", "value": "\\a+"}
  }
}
