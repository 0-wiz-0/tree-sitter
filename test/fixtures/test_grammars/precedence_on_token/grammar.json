{
  "name": "precedence_on_token",

  "extras": [
    {"type": "SYMBOL", "name": "comment"},
    {"type": "PATTERN", "value": "\\s"},
  ],

  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "string"
      }
    },

    "comment": {
      "type": "PATTERN",
      "value": "//.*"
    },

    "string": {
      "type": "SEQ",
      "members": [
        {"type": "STRING", "value": "\""},

        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "TOKEN",
                "content": {
                  "type": "PREC",
                  "value": 1,
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\"\n\\\\]+"
                  }
                }
              },
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              }
            ]
          }
        },

        {"type": "STRING", "value": "\""}
      ]
    },

    "escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\."
    }
  }
}
